{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@styles/*": ["./src/styles/*"],
      "@assets/*": ["./src/assets/*"],
      "@content/*": ["./src/content/*"],
      "@scripts/*": ["./src/scripts/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "astro"
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}